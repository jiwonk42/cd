<h1>Cd</h1>
<form action="add-cd" method="post">
  <label for="artist">Enter the name of the artist: </label>
  <input id="artist" type="text" name="artist">
  <label for="album">Enter the name of the album: </label>
  <input id="album" type="text" name="album">
  <button type="submit" name="button">Add</button>
</form>

<h1>Current CDs:</h1>
<ul>
  @foreach(var cd in Model)
  {
    <li><a href="/cd/@cd.GetId()">@cd.GetArtist() - @cd.GetAlbum() </a></li>
  }

</ul>
